(setf my-stumpish-path "/home/feng/project/stumpwm/contrib/stumpish")
(setf my-swank-loader "~/project/slime/swank-loader.lisp")
(setf debian-menu-file "/usr/share/common-lisp/source/stumpwm/contrib/stumpwm.menu")
